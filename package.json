{
  "name": "n8n-nodes-ytube-transcript-wlang",
  "version": "1.1.7",
  "description": "Custom n8n node to fetch YouTube subtitles with language and cookie support using yt-dlp",
  "main": "index.js",
  "type": "commonjs",
  "author": "minproducer",
  "license": "MIT",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "youtube",
    "transcript",
    "yt-dlp",
    "subtitles",
    "custom-node",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minproducer/n8n-nodes-ytube-transcript-wlang.git"
  },
  "bugs": {
    "url": "https://github.com/minproducer/n8n-nodes-ytube-transcript-wlang/issues"
  },
  "homepage": "https://github.com/minproducer/n8n-nodes-ytube-transcript-wlang#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "nodes/YouTubeTranscriptWithLanguage/YouTubeTranscriptWithLanguage.node.js"
    ]
  },
  "dependencies": {
    "n8n-core": "^1.61.0",
    "n8n-workflow": "^1.61.0",
    "yt-dlp-wrap": "^2.3.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "eslint": "^8.57.1",
    "fs-extra": "^11.2.0",
    "globals": "^16.2.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "node tools/test-runner.js",
    "test:cli": "node tools/test-runner.js",
    "test:video": "node tools/test-runner.js --video dQw4w9WgXcQ --save",
    "test:batch": "node tools/test-runner.js --file tools/video-list.json --save"
  },
  "files": [
    "nodes/",
    "index.js",
    "icon.svg",
    "README.md",
    "LICENSE"
  ]
}
